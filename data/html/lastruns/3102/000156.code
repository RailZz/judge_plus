#include <iostream>
#include <vector>
#include <cmath>
using namespace std;

bool f(long long m, long long a, long long b, long long w, long long h, long long n)
{
	return n >= (w / (a + 2 * m)) * (h / (b + 2 * m));
}

int main()
{
	long long n, a, b, w, h;
	cin >> n >> a >> b >> w >> h;
	long long l, r;
	l = 0;
	r = pow(10.0, 18);
	while (r - l > 1)
	{
		long long m = (l + r) / 2;
		if (f(m,a,b,w,h,n))
		{
			l = m;
		} else {
			r = m;
		}
	}
	cout << l;
	return 0;
}