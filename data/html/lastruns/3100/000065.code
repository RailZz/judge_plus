#include <iostream>
#include <math.h>
using namespace std;
int main ()
{
    int x,rez,z=2;
    cin >> x;
    if (x%2==0&&x!=2)
        cout << "composite";
    else
    {
        for (int i=2;i<=sqrt(x);i++)
        {
            if (x%i==0)
            {
                z++;
                break;
            }
        }
    }
    if (z==3)
        cout << "composite";
    if (z==2&&x%2==1||x==2)
        cout << "prime";

}