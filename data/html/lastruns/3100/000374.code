def is_prime(q):
    e = 0
    for i in range(2, int(q ** 0.5) + 1):
        if q % i == 0:
            e += 1
    if e == 0:
        return True
    else:
        return False


n = int(input())
ans = []

for i in range(2, n):
    if is_prime(i) == True and is_prime(n - i) == True:
        ans.append(i)
        ans.append(n - i)
        break

print(*ans)
         
