#include <stdio.h>
int main (void){
    int n;
    scanf ("%d", &n);
    int c = 0;
    int i = 2;
    while (i*i <= n){
            if (n % i == 0){
            c +=1;
         }
        i = i + 1;
    }

    if (c == 0 && n > 1){
        printf ("prime");
    }
    else {
        printf ("composite");
    }
    return 0;
}

