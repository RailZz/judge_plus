#include <iostream>
using namespace std;

int main(){
    unsigned long long a;
    cin >> a;
    if (a == 1)return 0;
    unsigned long long i;
    for (i = 2; i * i <= a; i++)
    {
        if (a % i == 0)
        {
            cout << "composite";
			system("pause");
            return 0;
        }
    }
    cout << "prime";
	system("pause");
    return 0;
}