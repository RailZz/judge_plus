#include <iostream>

using namespace std;

int main()
{
    int n, fact = 1, k = 0;
    cin >> n;
    for (int i = 1; i <= n; i++)
    {
        fact = fact*i;
    }
    for (int i = 1; i <= fact; i++)
    {
        if (fact%i == 0)
        {
            k++;
        }
    }
    cout << k;
    return 0;
}