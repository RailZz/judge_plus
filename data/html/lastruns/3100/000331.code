#include <iostream>
#include <vector>

using namespace std;
unsigned long long fact(int n)
{
    unsigned long long s = 1;
    while (n > 1)
    {
       s *= n;
       n--;
    }
    return s;
}

int main()
{
    int n, Count;
	long long ans;
    Count = 0;
    cin >> n;
    ans = fact(n);
    for (int i = 1; i <= ans; i++)
    {
        if (ans % i == 0)
        {
            Count++;
        }
    }
    cout << ans;
	system("pause");
    return 0;
}