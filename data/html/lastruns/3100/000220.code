#include <iostream>
using namespace std;

int main(){
    unsigned long long a;
    cin >> a;
    if (a > 2000000000)return 0;
    if (a == 1)return 0;
    unsigned long long i;
    for (i = 2; i <= a/2; i++)
    {
        if (a % i == 0)
        {
            cout << "composite";
            return 0;
        }
    }
    cout << "prime";
    return 0;
}