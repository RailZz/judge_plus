#include <iostream>    
using namespace std;

int IsSimple(long n)
{
    long  i; 
    for (int i = 2; i * i <= n; i++)
    {
        if ((n % i) == 0)
		{
			return 0;
		}
    }
    return 1;
}

int main()    
{    
    long  n, i, k;
    cin >> n;
    for (i = 2; i <= (n / 2); i++)
    {
		if (IsSimple(i) && IsSimple(n - i))
        {
         cout << i << " " << (n - i); 
         break;
        }
     }
     return 0;
}