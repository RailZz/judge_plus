#include <iostream>
#include <cmath>

using namespace std;

int main(){

const double PI = 3.1415926535897932384626433832795;
double a;
double l = -PI/2;
double r = PI/2;
double m = 0;
cin>> a;
for(int i = 0; i<100;i++){
    m = (l+r)/2;
    if(sin(m)<a){
        l = m;
        }else{
        r = m;
        }
    }
    cout<< l;
    return(0);
}
