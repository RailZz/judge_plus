#define _USE_MATH_DEFINES
#include <stdio.h>
#include <math.h>

int main(void){
    double l, r, a, m;
    scanf("%f", &a);
    l = -1.57079632679489661923;
    r = 1.57079632679489661923;
    for (int i = 0; i < 50; i +=1){
      m = (r - l)/ 2;

      if (sin(m) < a) {
           l = m;
      }else{
          r = m;
      }
    }
    printf("%.12f", m);
    return 0;
}
