#include <iostream>
#include <cmath>

using namespace std;

double f(double x)
{
    return x * x + sqrt(x);
}

int main()
{
    double c;
    cin >> c;
    double l, r, m;
    while (r - l > pow(10, -5))
    {
        l = 0;
        r = pow(10, 5);
        m = (l + r) / 2;
        if (f(m) < c)
        {
            l = m;
        } else {
            r = m;
        }
    }
    cout.precision(20);
    cout << l;
    return 0;
}