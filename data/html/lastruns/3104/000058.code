#include<iostream>
using namespace std;
int main()
{
    int n,i;
    cin>>n;
    if(n>0&&n<31)
    {
        int d[n];
        d[0]=1;
        d[1]=1;
        d[2]=2;
        if(n>=3){
        for(i=3;i<=n;i++){
            d[i]=d[i-1]+d[i-2]+d[i-3];
        }
        cout<<d[i-1];
}
    else{cout<<d[n];}
}
}
