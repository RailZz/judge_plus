#include <bits/stdc++.h>

using namespace std;

int main()
{
    long long n, k;
    cin >> n;
    vector<long long> a(n + 1);
    int i;
    a[0] = 0;
    for (i = 1; i <= n; ++i)
    {
        cin >> a[i];
        if (i != 1)
        {
            a[i] += a[i - 1];
        }
    }
    cin >> k;
    int left, right;
    for (i = 0; i < k; ++i)
    {
        cin >> left >> right;
        cout << a[right] - a[left - 1] << " ";
    }
}
