

def f(a,b,c):
    x = 0
    y = 0
    d = [a,b,c]
    for i in range(3):
        if (d[i] == 0):
            x += 1
        else:
            y +=1
    if (x > y):
        p = 0
        return p
    else:
        p = 1
        return p
n = input()
m = n.replace(' ','')
s = list(m)

v = f(s[0],s[1],s[2])
print(v)



