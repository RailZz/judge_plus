def int_min (a, b, c, d):
    if a<=b and a<=c and b<=d:
        return a
    else:
        if b<=a and b<=c and b<=d:
            return b
        else:
            if c<=a and c<=b and c<=d:
                return c
            else:
                return d
a = int(input())
b = int(input())
c = int(input())
d = int(input())
print(int_min(a, b, c, d))
