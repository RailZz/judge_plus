def schitivalka():
    a, b, c, d = [int(i)for i in input().split()]
    return a, b, c, d
def faser(a, b, c, d):
    if a<=b and a<=c and a<=d:
        return a
    elif b<=a and b<=c and b<=d:
        return b
    elif c<=a and c<=b and c<=d:
        return c
    else:
        return d
a, b, c, d = schitivalka()
print(faser(a, b, c, d))
    
