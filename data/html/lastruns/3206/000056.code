a = input()
b = 0
c = 0
e = 0
s = 0
for i in range(len(a)) :
    s += 1
    if ((ord(a[i]) >= 97) and (ord(a[i]) <= 122)):
        b += 1
    elif ((ord(a[i]) >= 65) and (ord(a[i]) <= 90)):
        c += 1
    else:
        e += 1
if  (((e > 0) and ( b > 0) and (c > 0 ) ) and (s >= 8)):
    print('YES')
else:
    print('NO')



