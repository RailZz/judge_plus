def find_min(a, b, c, d):
    if a<=b and a<=c and a<=b:
        return a
    elif b<=a and b<=c and b<=d:
        return b
    elif c<=a and c<=b and c<=d:
        return c
    elif d<=a and d<=b and d<=c:
        return d
    
N = int(input())
M = int(input())
l = int(input())
s = int(input())
if N<M:
    a = N - l
    b = M - s
else:
    a = M - l
    b = N - s

print (find_min(a, b, l, s))
