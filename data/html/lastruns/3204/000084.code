#include<iostream>
#include<cstdio>
#include<vector>
using namespace std;
int main(){
	vector<int> a;
	int h,k = 0;
	while(scanf("%d ",&h)==1){
		a.push_back(h);
	}
	for(int i = 0; i < a.size();++i){
		cerr << a[i] << ' ';
	}
	for(int i = 0; i < a.size();++i){
		if(a[i]>0){
			k++;
		}
	}
	cout << k << endl;
}